{
    "name": "iamshanto/lumen-passport",
    "description": "Fork Laravel Passport support to mongodb.",
    "keywords": ["laravel", "passport", "oauth", "mongodb"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samiul Amin",
            "email": "iamshanto@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "firebase/php-jwt": "^5.0",
        "illuminate/auth": "^8.2",
        "illuminate/console": "^8.2",
        "illuminate/container": "^8.2",
        "illuminate/contracts": "^8.2",
        "illuminate/cookie": "^8.2",
        "illuminate/database": "^8.2",
        "illuminate/encryption": "^8.2",
        "illuminate/http": "^8.2",
        "illuminate/support": "^8.2",
        "lcobucci/jwt": "^3.4|^4.0",
        "league/oauth2-server": "^8.2",
        "nyholm/psr7": "^1.3",
        "phpseclib/phpseclib": "^2.0|^3.0",
        "symfony/psr-http-message-bridge": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Lumen\\Passport\\": "src/",
            "Lumen\\Passport\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lumen\\Passport\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "providers": [
                "Lumen\\Passport\\PassportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
